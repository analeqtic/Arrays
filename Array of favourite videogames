using System;

namespace Program
{
    class Program
    {
        static void Main(string[] args)
        {
            string[] videogames = {"Rust", "League of legends", "Valorant", "Project Playtime"};

            for (int i = 0; i < videogames.Length; i++)
            {
                int rank = i + 1;
                Console.WriteLine(rank + ". " + videogames[i] );
            }
            
            Console.ReadKey();
        }
    }
}
         
